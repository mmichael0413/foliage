{{#rows}}
	<div class="item pure-g alert-row">
		<p class="col-4-12 col-md-4-12">{{title}}</p>
		<p class="col-3-12 col-md-3-12">{{created_at}}</p>
		<p class="col-3-12 col-md-3-12">{{resolved_at}}</p>
		<div class="col-1-12">
			<a href="{{links.resolve}}" class="btn primary expand">...</a>
		</div>
		<div class="col-1-12">
			<a href="{{links.checkin}}" class="btn primary"><i class="ic fa ic_report-16"></i></a>
		</div>
		{{> 'alert_details_empty_row'}}
	</div>
{{else}}
	<p>There are no Resolved Alerts for this Store</p>
{{/rows}}