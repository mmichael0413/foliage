<div class="widget {{config.widget_class}}">
    <h3>{{title}}</h3>
    {{#each results.rows}}
        <div class="leading-dots leading-row">
            <span class="pull-right">{{config.prepend_count}}{{sum}}{{config.append_count}}</span>
            <span>{{title}}</span>
        </div>
    {{/each}}
</div>