<div class="col-5-5">
  {{#if jobDetail}}
    <div><strong>{{jobDetail}}</strong></div>
  {{/if}}
    <div>{{storeName}}: {{street}} {{city}}, {{state}}</div>
    <b>{{dateScheduled}}</b>
    <input type="hidden" class="visit" value="{{id}}"/>
    <input type="text" value="{{dateScheduled}}" style="display: none;"/>
    {{#if canUnassign}}<a href="#" class='unassign btn primary small solid '>Unassign Visit</a>{{/if}}
</div>