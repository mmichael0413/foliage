<input type="text" class="low-margin" name="residentialAddress.city" placeholder="City"
       value="{{address.city}}" data-rule-required="true"/>
<select name="residentialAddress.state" data-rule-required="true">
    <option value="">Select State/Province</option>
    {{#each ../states}}
        {{#select ../address.state}}
            <option value="{{this.abbr}}">{{this.name}}</option>
        {{/select}}
    {{/each}}
</select>